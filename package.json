{
  "name": "tinty",
  "description": "Sketch Plugin that lets you create tints and shades of any color as shared styles and document colors.\n",
  "version": "0.1.1-0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Tinty",
    "manifest": "src/manifest.json",
    "main": "tinty.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4",
    "@skpm/extract-loader": "^2.0.2",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "author": "chewbecca",
  "dependencies": {
    "css-loader": "^3.4.1",
    "extract-loader": "^3.1.0",
    "html-loader": "^0.5.5",
    "prettier": "^1.19.1",
    "sketch-module-web-view": "^3.4.1"
  }
}
